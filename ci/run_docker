docker run \
  -p 3000:3000 \
  -e MONGOLAB_URI=$MONGOLAB_URI \
  -e MYSQL_URL=$MYSQL_URL \
  -e REDIS_PORT_6379_TCP_ADDR=$REDIS_PORT_6379_TCP_ADDR \
  -e REDIS_PORT_6379_TCP_PORT=$REDIS_PORT_6379_TCP_PORT \
  -e LOGGLY_SUBDOMAIN=$LOGGLY_SUBDOMAIN \
  -e LOGGLY_TOKEN=$LOGGLY_TOKEN \
  sentia/web:latest >> $CIRCLE_ARTIFACTS/docker.log
