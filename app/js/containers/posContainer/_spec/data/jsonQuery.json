{
  "fields": {
    "sum(revenue)": "revenue",
    "sum(transactions)": "transactions",
    "time": "time"
  },
  "where": {
    "store": 1,
    "date(time)": {
      "gte": "1970-01-01",
      "lte": "1970-01-01"
    }
  },
  "groupBy": ["date(time), hour(time)"],
  "orderBy": {
    "time": true
  }
}
